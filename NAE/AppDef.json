{
    "name": "app_plot",
    "description": "app_plot application for Gnuplot",
    "author": "jhegge",
    "licensed": true,
    "classifications": [
        "Software Development"
    ],
    "machines": [
        "n*"
    ],
    "vault-types": [
        "FILE",
        "BLOCK",
        "BLOCK_ARRAY",
        "OBJECT"
    ],
    "commands": {
        "Batch": {
            "path": "/usr/bin/gnuplot",
            "interactive": false,
            "name": "Batch",
            "description": "Run Gnuplot on sample data",
            "parameters": {
                "commands-file": {
                    "name": "Commands-file",
                    "description": "file of Gnuplot commands to run",
                    "type": "FILE",
                    "positional": true,
                    "required": true
                }
            }
        },
        "Server": {
            "path": "/sbin/init",
            "interactive": true,
            "name": "Server",
            "description": "Launch a session with all boot services, including SSH (if installed).  Connection address and credentials will appear in your web browser once available.",
            "parameters": {}
        }
    },
    "image": {
        "data": "",
        "type": "image/png"
    }
}